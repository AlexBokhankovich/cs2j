<?xml version="1.0" encoding="utf-8"?>
<Class xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="file://C:/Documents%20and%20Settings/kevin.glynn/My%20Documents/CS2JLibrary/Translation.xsd"
 xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>System.IO.Path</Name>
  <Properties>
    <Property>
       <Type>System.Char</Type>
       <Name>DirectorySeparatorChar</Name>
       <Get>File.separatorChar</Get>
       <Imports>
         <Import>java.io.File</Import>
       </Imports>
    </Property>
  </Properties>
  <Methods>
    <Method>
      <Return>System.String</Return>
      <Name>GetDirectoryName</Name>
      <Params>
        <Param>
          <Type>System.String</Type>
          <Name>path</Name>
        </Param>
      </Params>
	  <Imports>
	    <Import>java.io.File</Import>
	  </Imports>
      <Java>(new File(${path})).getParent()</Java>
    </Method>
    <Method>
      <Return>System.String</Return>
      <Name>GetFileName</Name>
      <Params>
        <Param>
          <Type>System.String</Type>
          <Name>path</Name>
        </Param>
      </Params>
	  <Imports>
	    <Import>java.io.File</Import>
	  </Imports>
      <Java>(new File(${path})).getName()</Java>
    </Method>
    <Method>
      <Return>System.String</Return>
      <Name>GetFullPath</Name>
      <Params>
        <Param>
          <Type>System.String</Type>
          <Name>path</Name>
        </Param>
      </Params>
	  <Imports>
	    <Import>java.io.File</Import>
	  </Imports>
      <Java>(new File(${path})).getCanonicalPath()</Java>
    </Method>
    <Method>
      <Return>System.String</Return>
      <Name>GetExtension</Name>
      <Params>
        <Param>
          <Type>System.String</Type>
          <Name>path</Name>
        </Param>
      </Params>
	  <Imports>
	    <Import>RusticiSoftware.System.IO.PathSupport</Import>
	  </Imports>
      <Java>PathSupport.getExtension(${path})</Java>
    </Method>
    <Method>
      <Return>System.String</Return>
      <Name>Combine</Name>
      <Params>
        <Param>
          <Type>System.String</Type>
          <Name>path1</Name>
        </Param>
        <Param>
          <Type>System.String</Type>
          <Name>path2</Name>
        </Param>
      </Params>
	  <Imports>
	    <Import>java.io.File</Import>
	  </Imports>
      <Java>(new File(${path1}, ${path2})).toString()</Java>
    </Method>
  </Methods>
</Class>