<?xml version="1.0" encoding="utf-8"?>
<Class xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="file://C:/Documents%20and%20Settings/kevin.glynn/My%20Documents/CS2JLibrary/Translation.xsd"
 xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>System.IO.TextWriter</Name>
  <Inherits>
    <Type>System.IDisposable</Type>
  </Inherits>
  <Methods>
    <Method>
      <Return>System.Void</Return>
      <Name>Close</Name>
      <Java>${this}.close()</Java>
    </Method>
    <Method>
      <Return>System.Void</Return>
      <Name>WriteLine</Name>
      <Params>
        <Param>
          <Type>System.String</Type>
          <Name>arg</Name>
        </Param>
      </Params>
      <Java>${this}.write(${arg} + System.getProperty("line.separator"))</Java>
    </Method>
   <Method>
      <Return>System.Void</Return>
      <Name>Write</Name>
      <Params>
        <Param>
          <Type>System.String</Type>
          <Name>arg</Name>
        </Param>
      </Params>
      <Java>${this}.write(${arg})</Java>
    </Method>
  </Methods>
</Class>